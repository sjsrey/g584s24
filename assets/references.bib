@article{freeman2019VehicleStackingEstimation,
  title = {Vehicle Stacking Estimation at Signalized Intersections with Unmanned Aerial Systems},
  author = {Freeman, Brian S. and Al Matawah, Jamal Ahmad and Najjar, Musaed Al and Gharabaghi, Bahram and Th{\'e}, Jesse},
