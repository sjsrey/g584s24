@article{neelon2013SpatialPoissonHurdle,
  title = {A Spatial {{Poisson}} Hurdle Model for Exploring Geographic Variation in Emergency Department Visits: {{{\emph{Spatial Hurdle Model}}}}{\emph{ for }}{{{\emph{Exploring Geographic Variation}}}}},
