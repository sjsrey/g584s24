@article{cutsinger2005VerifyingMultiDimensionalNature,
  title = {Verifying the {{Multi-Dimensional Nature}} of {{Metropolitan Land Use}}: {{Advancing}} the {{Understanding}} and {{Measurement}} of {{Sprawl}}},
  shorttitle = {Verifying the {{Multi-Dimensional Nature}} of {{Metropolitan Land Use}}},
  author = {Cutsinger, Jackie and Galster, George and Wolman, Harold and Hanson, Royce and Towns, Douglas},
  year = {2005},
