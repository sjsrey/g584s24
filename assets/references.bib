  shorttitle = {Measuring the {{Degree}} of {{Overlap}} and {{Segregation}} among {{Multiple Probabilistic Home Ranges}}},
  author = {Ferrarini, Alessandro and Giglio, Giuseppe and Pellegrino, Stefania Caterina and Gustin, Marco},
