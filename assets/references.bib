@article{nyberg2018NetworkGTGISTool,
  title = {{{NetworkGT}}: {{A GIS}} Tool for Geometric and Topological Analysis of Two-Dimensional Fracture Networks},
  shorttitle = {{{NetworkGT}}},
  author = {Nyberg, Bj{\"o}rn and Nixon, Casey W. and Sanderson, David J.},
