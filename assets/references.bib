@article{fleischmann2022MethodologicalFoundationNumerical,
  title = {Methodological Foundation of a Numerical Taxonomy of Urban Form},
  author = {Fleischmann, Martin and Feliciotti, Alessandra and Romice, Ombretta and Porta, Sergio},
  year = {2022},
  month = may,
  journal = {Environment and Planning B: Urban Analytics and City Science},
  volume = {49},
