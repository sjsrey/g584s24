@article{hedlund2006MigratingTwinsStudy,
  title = {Migrating {{Twins}}: {{A Study}} of {{Welfare Components Among Finnish Twins Living}} in {{Sweden}} and Their {{Co-Twins Residing}} in {{Finland}}},
  shorttitle = {Migrating {{Twins}}},
  author = {Hedlund, Ebba and Hammar, Niklas and Lange, Anders and Koskenvuo, Markku and J{\"o}rgensen, Lena and Kaprio, Jaakko},
  year = {2006},
  month = jan,
  journal = {Journal of Ethnic and Migration Studies},
  volume = {32},
  number = {1},
  pages = {3--28},
  publisher = {{Routledge}},
  issn = {1369-183X},
  doi = {10.1080/13691830500335333},
  urldate = {2021-08-31},
  abstract = {Migration is often undertaken to improve living conditions and to increase access to welfare resources. This study aims to analyse to what extent migration from Finland to Sweden has influenced major welfare components among migrants, including employment, socio-economic status and social support. The study population consisted of 1,083 twin pairs of the Finnish Twin Cohort, where at least one twin of the pair had emigrated to Sweden. Information about welfare and other factors was obtained by a postal questionnaire survey undertaken in 1998. In the analysis, emigrants and returnees were compared to non-migrants residing in Finland. Pairwise comparisons were performed in migration-discordant twin pairs. Though we found a somewhat higher educational level among emigrants compared to those who had never migrated, the emigrants{\textemdash}and especially the women{\textemdash}were more usually engaged in manual work. Compared to their co-twins in Finland, migrants were more likely to be employed in 1998 but considered themselves more often overqualified for their job. Male emigrants were less often married or cohabitating than never migrants, and their social relations were more often characterised by a lack of attachment; the opposite was true for female emigrants. Migration from Finland to Sweden does not seem to have substantially improved the socio-economic position among emigrants. In part, this could be due to difficulties in finding a job commensurate with their level of qualification. Migration seems to have favourably influenced social support among emigrant women but not among emigrant men.},
  keywords = {Finland,Integration,Migration,Studies of Twins,Sweden,Welfare},
  file = {/Users/srey/Zotero/storage/SJZNLU73/Hedlund et al_2006_Migrating Twins.pdf;/Users/srey/Zotero/storage/ZTYWI8BJ/13691830500335333.html}
}

