@article{quick2013ExploringHotspotsDrug,
  title = {Exploring {{Hotspots}} of {{Drug Offences}} in {{Toronto}}: {{A Comparison}} of {{Four Local Spatial Cluster Detection Methods}}},
  shorttitle = {Exploring {{Hotspots}} of {{Drug Offences}} in {{Toronto}}},
  author = {Quick, Matthew and Law, Jane},
  year = {2013},
  month = apr,
  journal = {Canadian Journal of Criminology and Criminal Justice},
  volume = {55},
  number = {2},
  pages = {215--238},
  issn = {1707-7753, 1911-0219},
  doi = {10.3138/cjccj.2012.E13},
  urldate = {2022-09-25},
  abstract = {Spatial cluster detection is an exploratory spatial data analysis technique that identifies areas or groups of areas with disproportionately high risk. Several local cluster detection methods have been developed; yet no research has critiqued these methods as they contribute to spatial studies of crime. This study aims to identify the locations of drug offence hotspots in Toronto and compare the clusters detected through four methods: (1) spatial scan statistic {\textendash} Euclidean distance, (2) spatial scan statistic {\textendash} non-Euclidean contiguity, (3) flexibly shaped scan statistic, and (4) local Moran's I. It was found that all methods detected clusters in the downtown, with fewer methods detecting clusters in the west and east of Toronto. It was observed that the spatial scan statistic detected the largest and most circular clusters, making it a suitable tool to inform general policing initiatives and highlight possible variables to be included in confirmatory research. The local Moran's I method, in contrast, found the smallest and most compact clusters, indicating that it is an appropriate test for identifying areas where resource intensive crime prevention and policing efforts should be targeted.},
  langid = {english}
}

