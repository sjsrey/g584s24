@article{gastwirth2014MedianbasedMeasuresInequality,
  title = {Median-Based Measures of Inequality: {{Reassessing}} the Increase in Income Inequality in the {{U}}.{{S}}. and {{Sweden}}},
  shorttitle = {Median-Based Measures of Inequality},
  author = {Gastwirth, Joseph L.},
  year = {2014},
  month = dec,
  journal = {Statistical Journal of the IAOS},
  volume = {30},
