@article{Reardon2008,
  title = {The Geographic Scale of Metropolitan Racial Segregation},
  author = {Reardon, Sean F. and Matthews, Stephen A. and O'Sullivan, David and Lee, Barrett A. and Firebaugh, Glenn and Farrell, Chad R. and Bischoff, Kendra},
  year = {2008},
  journal = {Demography},
  volume = {45},
  number = {3},
  pages = {489--514},
  issn = {1533-7790},
  doi = {10.1353/dem.0.0019},
  abstract = {This article addresses an aspect of racial residential segregation that has been largely ignored in prior work: the issue of geographic scale. In some metropolitan areas, racial groups are segre- gated over large regions, with predominately white regions, predominately black regions, and so on, whereas in other areas, the separation of racial groups occurs over much shorter distances. Here we develop an approach{\textemdash}featuring the segregation profi le and the corresponding macro/micro segregation ratio{\textemdash}that offers a scale-sensitive alternative to standard methodological practice for describing segregation. Using this approach, we measure and describe the geographic scale of racial segregation in the 40 largest U.S. metropolitan areas in 2000. We fi nd considerable heterogeneity in the geographic scale of segregation patterns across both metropolitan areas and racial groups, a heterogeneity that is not evident using conventional ``aspatial'' segregation measures. Moreover, be- cause the geographic scale of segregation is only modestly correlated with the level of segregation in our sample, we argue that geographic scale represents a distinct dimension of residential segregation. We conclude with a brief discussion of the implications of our fi ndings for investigating the patterns, causes, and consequences of residential segregation at different geographic scales},
  bdsk-url-2 = {https://doi.org/10.1353/dem.0.0019}
}

