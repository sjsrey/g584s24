  author = {Alrashid, Hussah and Magdy, Amr and Rey, Sergio},
  year = {2022},
  abstract = {Regionalization is the process of grouping a collection of spatial areas into spatially contiguous regions. This paper presents a novel spatial regionalization problem, termed SIRC (Scalable Statistical Inference for Spatial Regionalization with Cardinality Constraints), which involves organizing a set of spatial areas into a specific number of regions (i.e., p regions), with each region having a predefined cardinality. The primary motivation behind SIRC is to enable users to conduct statistical inference on any existing regionalization scheme by generating sample solutions that serve as a reference distribution. To address SIRC, we introduce a parallel technique named PRRP (P-Regionalization through Recursive Partitioning). PRRP recursively divides input areas into two parts: (1) one region with a predefined cardinality, and (2) the remaining unassigned input areas. This recursive partitioning is executed over three phases: the region growing phase constructs initial regions with a predefined cardinality, while the region merging and region splitting phases ensure the spatial contiguity of input areas, allowing subsequent regions to grow and achieve their predefined size. An extensive experimental evaluation demonstrates the effectiveness of PRRP in solving SIRC using several real-world datasets.},
  langid = {english},
  file = {/Users/srey/Zotero/storage/4UILEZNJ/Alrashid et al. - 2022 - SIRC Scalable Statistical Inference for Spatial R.pdf}
}

