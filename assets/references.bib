  author = {J{\"a}rvinen, Teppo L. N. and Sihvonen, Raine and Bhandari, Mohit and Sprague, Sheila and Malmivaara, Antti and Paavola, Mika and Sch{\"u}nemann, Holger J. and Guyatt, Gordon H.},
