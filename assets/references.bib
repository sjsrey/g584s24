  title = {Population Shift Bias in Tests of Space{\textendash}Time Interaction},
  author = {Mack, Elizabeth A. and Malizia, Nicholas and Rey, Sergio J.},
  year = {2012},
  month = nov,
  journal = {Computers, Environment and Urban Systems},
