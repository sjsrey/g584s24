}

@article{wu2019EnvironmentalRegulationsRedistribution,
  title = {Environmental Regulations and Redistribution of Polluting Industries in Transitional {{China}}: {{Understanding}} Regional and Industrial Differences},
  shorttitle = {Environmental Regulations and Redistribution of Polluting Industries in Transitional {{China}}},
  author = {Wu, Jiawei and Wei, Yehua Dennis and Chen, Wen and Yuan, Feng},
