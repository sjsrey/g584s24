@article{zhou2021BiObjectiveOptimizationBattery,
  title = {Bi-{{Objective Optimization}} for {{Battery Electric Bus Deployment Considering Cost}} and {{Environmental Equity}}},
  author = {Zhou, Yirong and Liu, Xiaoyue Cathy and Wei, Ran and Golub, Aaron},
